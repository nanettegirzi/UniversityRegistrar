@{
  Layout="_Layout";
}
<br>
<div class="container-fluid">
@if (Model.Count != 0)
{
  <ul class="list-group">
    @foreach (var course in Model)
    {
      <li class="list-group-item"><a href="/courses/@course.GetId()">@course.GetCourseName()</a>  -      <a href="/courses/@course.GetId()/update">Edit</a> | <a href="/courses/@course.GetId()/delete">Delete</a></li>
    }
  </ul>
}
<br>
<hr>
<br>
<h4><a href="/courses/new">Add a new course</a></h4>
</div>
</div>
